name=Willy
MO_ALLOW_FUNCTION_ARGUMENTS=true

pipeTo() {
    echo -n "$1" | "$2"
}

testArgs() {
    printf "%d" "$#"

    # Remove content. Note that when zero arguments are passed, this
    # line does nothing and $1 will still be the content.
    shift

    # Display all arguments
    printf " %q" "$@"
}
